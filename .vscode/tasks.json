{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc.exe 生成活动文件",
            "command": "D:/SDK/w64devkit/bin/gcc.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g", // 生成调试信息
                "${file}", // 当前打开的.c文件
                "-o", // 输出文件选项
                "${fileDirname}\\build\\${fileBasenameNoExtension}.exe", // 输出路径
                ""
            ],
            "options": {
                "cwd": "D:/SDK/w64devkit/bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}